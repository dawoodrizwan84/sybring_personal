@model List<sybring_personal.Models.User>

@{
    ViewData["Title"] = "Users List";
}

<h1>Users List</h1>

<table class="table">
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
            <th></th>
            
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
               
            </tr>
        }
    </tbody>
</table>
